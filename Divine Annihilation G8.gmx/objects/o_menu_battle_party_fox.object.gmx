            <string>/// Scroll through active members


//==========================//
// Member Scroll Stuff
//==========================//
var kLeft, kRight, kSelect, kBack;

kLeft = keyboard_check_pressed(vk_left);
kRight = keyboard_check_pressed(vk_right);
kSelect = keyboard_check_pressed(ord("X"));
kBack = keyboard_check_pressed(ord("Z"));

if global.menuactive = 1
{
    if(!kLeft &amp;&amp; kRight)
    {
        if global.partymember1 = 1
        {
            if image_index = 0
            {
                image_index = 1;
                o_menu_battle_party_cat.image_index = 0;
                o_menu_battle_party_dog.image_index = 1;
                active = 0;
                alarm[0] = 10
            }
        }
        
        if active = 1
        {
            if global.partymember2 = 1
            {
                if o_menu_battle_party_cat.image_index = 0
                {
                    image_index = 1;
                    o_menu_battle_party_cat.image_index = 1;
                    o_menu_battle_party_dog.image_index = 0;
                }
            }
        }
    }
    
    if(kLeft &amp;&amp; !kRight)
