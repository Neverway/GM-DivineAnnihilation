//------------------------------------//
// Go back to main menu
//------------------------------------//
if global.keySecond
{
    instance_change(obj_menu_title_main, true);
}


//------------------------------------//
// Profile 1 selected
//------------------------------------//
if global.keyAction and image_index == 0
{
    if file_exists("profile1.sav")
