          <argument>
            <kind>1</kind>
            <string>/// Controls and movemnt

//==========================//
// Create Variables
//==========================//
var kUp, kDown, kLeft, kRight, kSelect, kBack, kMenu;


//==========================//
// Define Controls
//==========================//
kUp = keyboard_check(vk_up);
kDown = keyboard_check(vk_down);
kLeft = keyboard_check(vk_left);
kRight = keyboard_check(vk_right);
kSelect = keyboard_check_pressed(ord("X"));
kBack = keyboard_check_pressed(ord("Z"));
kMenu = keyboard_check_pressed(ord("C"));
bitdistance = 80
posmark_1 = o_system_posmark_1
posmark0 = o_system_posmark0
posmark1 = o_system_posmark1


//==========================//
// Movement
//==========================//


if canmove = 1
{
    if bitframed = 1
    {
    if kUp 
    {
        if lane = 0
        {
            bitframed = 0;
            y -= bitdistance;
            lane = -1
            alarm[5] = 15;
        } 
        if lane = 1
        {
            bitframed = 0;
            y -= bitdistance;
            lane = 0
            alarm[5] = 15;
        }
       
    }
    if kDown
    {
        if lane = 0
        {
            bitframed = 0;
            y += bitdistance;
            lane = 1
            alarm[5] = 15;
        } 
        if lane = -1
        {
            bitframed = 0;
            y += bitdistance;
            lane = 0
            alarm[5] = 15;
        }
       
    }
    }
}
</string>
