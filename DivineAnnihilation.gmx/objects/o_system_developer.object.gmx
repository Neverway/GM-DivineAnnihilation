        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Menu navigation and configuration

//==========================//
// Controls
//==========================//
var kSelect, kOpen
kSelect = keyboard_check_pressed(ord("Z"));
kOpen = keyboard_check_pressed(vk_f1);


//==========================//
// Drawing and centering
//==========================//
x = view_xview
y = view_yview + 5


//==========================//
// Toggleing the developer menu
//==========================//
if active
{
if kOpen
{

//--------------------------//
// Close devloper menu
//--------------------------//
    if global.developerMenuOpen == true
    {
        global.developerMenuOpen = false;
        image_index = 0;
        active = false;
        alarm[0] = 10;
    }
    
    
//--------------------------//
// Open devloper menu
//--------------------------//
    if global.developerMenuOpen == false
    {
        global.developerMenuOpen = true;
        image_index = 1;
        active = false;
        global.menuactive = 1;
        alarm[0] = 10;
    }

}
}

//==========================//
// Scrolling
//==========================//
if global.developerMenuOpen == true
{
    script_execute(scr_updown_menu, 2, 17)
}


//==========================//
// Selecting
//==========================//
if kSelect
{


//--------------------------//
// Toggle Nocliping
//--------------------------//
    if image_index == 2
    {
        if global.noclipEnabled == true
        {
            global.noclipEnabled = false;
            show_debug_message("False")
        }
        
        if global.noclipEnabled == false
        {
            global.noclipEnabled = true;
            
            show_debug_message("True")
        }
    }
    
    
//--------------------------//
// Toggle Meshing
//--------------------------//
    if image_index == 3
    {
        if global.meshingEnabled == true
        {
            global.meshingEnabled = false;
        }
        
        if global.meshingEnabled == false
        {
            global.meshingEnabled = true;
        }
    }
    
    
//--------------------------//
// Toggle Show Triggers
//--------------------------//
    if image_index == 4
    {
        if global.showTriggers == true
        {
            global.showTriggers = false;
        }
        
        if global.showTriggers == false
        {
            global.showTriggers = true;
        }
    }
    
    
//--------------------------//
// Next Room
//--------------------------//
    if image_index == 5
    {
        room_goto_next();
    }


//--------------------------//
// Previous Room
//--------------------------//
    if image_index == 6
    {
        room_goto_previous();
    }


//--------------------------//
// Reset Room
//--------------------------//
    if image_index == 7
    {
        room_restart();
    }


//--------------------------//
// Reset Game
//--------------------------//
    if image_index == 8
    {
        game_restart();
    }


//--------------------------//
// Add HP
//--------------------------//
    if image_index == 9
    {
        global.hp += 10;
    }
    
    
//--------------------------//
// Subtract HP
//--------------------------//
    if image_index == 10
    {
        global.hp -= 10;
    }
    
    
//--------------------------//
// Add Gold
//--------------------------//
    if image_index == 11
    {
        global.gold += 10;
    }
    
    
//--------------------------//
// Subtract Gold
//--------------------------//
    if image_index == 12
    {
        global.gold -= 10;
    }
    
    
//--------------------------//
// Add Level
//--------------------------//
    if image_index == 13
    {
        global.level += 1;
    }
    
    
//--------------------------//
// Subtract Level
//--------------------------//
    if image_index == 14
    {
        global.level -= 1;
    }
}
</string>
